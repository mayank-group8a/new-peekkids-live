<div class="img_txt_custom {% if section.settings.text_overlap != blank %} txt_overlap_sec {% endif %}" style="background:{{section.settings.bg_color}}">
    {%- if section.settings.hero_layout == 'fixed_width' -%}
        <div class="page-width">
      {%- endif -%}
      {% if section.settings.title != blank %}
        <div class="section-header text-center">
          <h4 class="h2">{{ section.settings.title | escape }}</h4>
        </div>
      {% endif %}
    
    
    
      <div class="custom-content overlap_sec">
        {%- assign block_width_mobile = '' -%}
    
        {% for block in section.blocks %}
          {% case block.settings.width %}
            {% when '25%' %}
              {%- assign max_height = 250 -%}
              {%- assign block_width = 'grid--4-col-tablet' -%}
              {%- assign block_width_mobile = 'grid--1-col' -%}
            {% when '33%' %}
              {%- assign max_height = 345 -%}
              {%- assign block_width = 'grid--3-col-tablet' -%}
              {%- assign block_width_mobile = 'grid--1-col' -%}
            {% when '50%' %}
              {%- assign max_height = 530 -%}
              {%- assign block_width = 'grid--2-col-tablet' -%}
              {%- assign block_width_mobile = 'grid--1-col' -%}
            {% when '100%' %}
              {%- assign max_height = 1090 -%}
              {%- assign block_width = 'grid--1-col' -%}
          {% endcase %}
        
           {% case block.settings.opacity_bg %}
          {% when 0 %}
            {%- assign overlay_opacity = 'opa_nil' -%}
          {% when 0.2 %}
            {%- assign overlay_opacity = 'opa_low' -%}
          {% when 0.4 %}
            {%- assign overlay_opacity = 'opa_midium' -%}
          {% when 0.6 %}
            {%- assign overlay_opacity = 'opa_high' -%}
          {% when 0.8 %}
            {%- assign overlay_opacity = 'opa_veryhigh' -%}
          {% when 1 %}
            {%- assign overlay_opacity = 'opa_solid' -%}
          {% endcase %}
    
          {%- if block.type == 'blog' or block.type == 'article' -%}
            {%- assign block_width_mobile = 'small--one-whole' -%}
          {%- endif -%}
    
          {% if block.type == 'image' %}
            {% capture img_id %}CustomImage--{{ forloop.index }}-{{ block.settings.image.id }}{% endcapture %}
            {% capture mobile_img_id %}CustomImage--{{ forloop.index }}-{{ block.settings.mobile_image.id }}{% endcapture %}
            {% capture img_wrapper_id %}CustomImageWrapper--{{ forloop.index }}-{{ block.settings.image.id }}{% endcapture %}
        {% capture mobile_img_wrapper_id %}CustomImageWrapper--{{ forloop.index }}-{{ block.settings.mobile_image.id }}{% endcapture %}
    
            {% unless block.settings.image == blank %}
                        {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
            {% endunless %}
          {% endif %}
    
            {%- assign text_alignments = block.settings.text_alignment | split: ' ' -%}
      {%- assign text_horizontal_alignment = text_alignments.first -%}
      {%- assign text_vertical_alignment = text_alignments.last | strip -%}
    
          <div class="custom__item custom__item--{{ block.type }} custom__item--{{ block.id }} {{ block_width_mobile }} {{ block_width }} {% if block.settings.text_alignment %}align--{{ text_vertical_alignment }} block-{{ text_horizontal_alignment }}_position{% endif %} {% if block.settings.alignment %} align--{{ block.settings.alignment }}{% endif %} {% if block.settings.text_position %}block-{{block.settings.text_position}}{% endif %}" {{ block.shopify_attributes }} >
            <div class="custom__item-inner custom__item-inner--{{ block.type }}"{% if block.type == 'image' %} id="{{ img_wrapper_id }}"{% endif %} >
              {% case block.type %}
                {% when 'image' %}
                    {%- assign img_url = block.settings.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
                    {%- assign mobile_img_url = block.settings.mobile_image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
                  {% if block.settings.image != blank %}                
                    <div class="img_cover {% if block.settings.mobile_image != blank %}small--hide{% endif %}" style="padding-top:{{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100}}%;" data-image-loading-animation>
                        <img
                        srcset="{%- if section.settings.image.width >= 375 -%}{{ section.settings.image | img_url: '375x' }} 375w,{%- endif -%}
                          {%- if section.settings.image.width >= 750 -%}{{ section.settings.image | img_url: '750x' }} 750w,{%- endif -%}
                          {%- if section.settings.image.width >= 1100 -%}{{ section.settings.image | img_url: '1100x' }} 1100w,{%- endif -%}
                          {%- if section.settings.image.width >= 1500 -%}{{ section.settings.image | img_url: '1500x' }} 1500w,{%- endif -%}
                          {%- if section.settings.image.width >= 1780 -%}{{ section.settings.image | img_url: '1780x' }} 1780w,{%- endif -%}
                          {%- if section.settings.image.width >= 2000 -%}{{ section.settings.image | img_url: '2000x' }} 2000w,{%- endif -%}
                          {%- if section.settings.image.width >= 3000 -%}{{ section.settings.image | img_url: '3000x' }} 3000w,{%- endif -%}
                          {%- if section.settings.image.width >= 3840 -%}{{ section.settings.image | img_url: '3840x' }} 3840w,{%- endif -%}"
                        sizes="{% if section.settings.mobile_image != blank and section.settings.stack_images_on_mobile %}(min-width: 750px) 50vw, 100vw{% elsif section.settings.mobile_image != blank %}50vw{% else %}100vw{% endif %}"
                        src="{{ section.settings.image | img_url: '1500x' }}"
                        loading="lazy"
                        alt="{{ section.settings.image.alt | escape }}"
                        width="{{ section.settings.image.width }}"
                        height="{{ section.settings.image.width | divided_by: section.settings.image.aspect_ratio }}"
                        {% if section.settings.mobile_image != blank %}class="banner__media-image-half"{% endif %}
                      >
                      <div class="overlay_div {{overlay_opacity}}" style="background:{{block.settings.overlay_color}}"></div>
                    </div>
                    <div class="banner__media media{% if section.settings.mobile_image == blank and section.settings.mobile_image == blank %} placeholder{% endif %}{% if section.settings.mobile_image != blank %} banner__media-half{% endif %}">
                        <img
                          srcset="{%- if section.settings.mobile_image.width >= 375 -%}{{ section.settings.mobile_image | img_url: '375x' }} 375w,{%- endif -%}
                            {%- if section.settings.mobile_image.width >= 750 -%}{{ section.settings.mobile_image | img_url: '750x' }} 750w,{%- endif -%}
                            {%- if section.settings.mobile_image.width >= 1100 -%}{{ section.settings.mobile_image | img_url: '1100x' }} 1100w,{%- endif -%}
                            {%- if section.settings.mobile_image.width >= 1500 -%}{{ section.settings.mobile_image | img_url: '1500x' }} 1500w,{%- endif -%}
                            {%- if section.settings.mobile_image.width >= 1780 -%}{{ section.settings.mobile_image | img_url: '1780x' }} 1780w,{%- endif -%}
                            {%- if section.settings.mobile_image.width >= 2000 -%}{{ section.settings.mobile_image | img_url: '2000x' }} 2000w,{%- endif -%}
                            {%- if section.settings.mobile_image.width >= 3000 -%}{{ section.settings.mobile_image | img_url: '3000x' }} 3000w,{%- endif -%}
                            {%- if section.settings.mobile_image.width >= 3840 -%}{{ section.settings.mobile_image | img_url: '3840x' }} 3840w,{%- endif -%}"
                          sizes="{% if section.settings.mobile_image != blank and section.settings.stack_images_on_mobile %}(min-width: 750px) 50vw, 100vw{% elsif section.settings.mobile_image != blank %}50vw{% else %}100vw{% endif %}"
                          src="{{ section.settings.image | img_url: '1500x' }}"
                          loading="lazy"
                          alt="{{ section.settings.image.alt | escape }}"
                          width="{{ section.settings.image.width }}"
                          height="{{ section.settings.image.width | divided_by: section.settings.image.aspect_ratio }}"
                          {% if section.settings.image_2 != blank %}class="banner__media-image-half"{% endif %}
                        >
                      </div>
                    <noscript>
                      {% capture image_size %}{{ max_height }}x{% endcapture %}
                      {{ block.settings.image | img_url: image_size, scale: 2, crop: 'top' | img_tag: block.settings.image.alt, 'custom__image' }}
                    </noscript>
                  {% else %}
                    {{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}
                  {% endif %}
                  {% if block.settings.mobile_image != blank %}
                  <div class="img_cover {% if block.settings.mobile_image != blank %}medium-up--hide{% endif %}" style="padding-top:{{ 1 | divided_by: block.settings.image.aspect_ratio | times: 100}}%;" data-image-loading-animation>
                    <img id="{{ mobile_img_id }}"
                         class="custom__image lazyload js"
                         data-src="{{ mobile_img_url }}"
                         data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
                         data-aspectratio="{{ block.settings.mobile_image.aspect_ratio }}"
                         data-sizes="auto"
                         alt="{{ block.settings.mobile_image.alt | escape }}">
                    <div class="overlay_div {{overlay_opacity}}" style="background:{{block.settings.overlay_color}}"></div>
                  </div>
                  <div class="banner__media media{% if section.settings.image == blank and section.settings.image_2 == blank %} placeholder{% endif %}{% if section.settings.image_2 != blank %} banner__media-half{% endif %}">
                    <img
                      srcset="{%- if section.settings.image.width >= 375 -%}{{ section.settings.image | img_url: '375x' }} 375w,{%- endif -%}
                        {%- if section.settings.image.width >= 750 -%}{{ section.settings.image | img_url: '750x' }} 750w,{%- endif -%}
                        {%- if section.settings.image.width >= 1100 -%}{{ section.settings.image | img_url: '1100x' }} 1100w,{%- endif -%}
                        {%- if section.settings.image.width >= 1500 -%}{{ section.settings.image | img_url: '1500x' }} 1500w,{%- endif -%}
                        {%- if section.settings.image.width >= 1780 -%}{{ section.settings.image | img_url: '1780x' }} 1780w,{%- endif -%}
                        {%- if section.settings.image.width >= 2000 -%}{{ section.settings.image | img_url: '2000x' }} 2000w,{%- endif -%}
                        {%- if section.settings.image.width >= 3000 -%}{{ section.settings.image | img_url: '3000x' }} 3000w,{%- endif -%}
                        {%- if section.settings.image.width >= 3840 -%}{{ section.settings.image | img_url: '3840x' }} 3840w,{%- endif -%}"
                      sizes="{% if section.settings.image_2 != blank and section.settings.stack_images_on_mobile %}(min-width: 750px) 50vw, 100vw{% elsif section.settings.image_2 != blank %}50vw{% else %}100vw{% endif %}"
                      src="{{ section.settings.image | img_url: '1500x' }}"
                      loading="lazy"
                      alt="{{ section.settings.image.alt | escape }}"
                      width="{{ section.settings.image.width }}"
                      height="{{ section.settings.image.width | divided_by: section.settings.image.aspect_ratio }}"
                      {% if section.settings.image_2 != blank %}class="banner__media-image-half"{% endif %}
                    >
                  </div>
                  {% endif %}
                {% when 'text' %}
                  
                  <div class="medium-up--text-{{ block.settings.align_text }} color_{{block.id}} {% comment %}w_{{block.settings.text_width}}{% endcomment %}"> 
                    {% if block.settings.title != blank %}
                      <h4 class="h3" style="color:{{block.settings.title_color}}">{{ block.settings.title | escape }}</h4>
                    {% endif %}
                    {% if block.settings.text != blank %}
                      <div class="rte-setting rte">{{ block.settings.text }}</div>
                    {% endif %}
                  </div>
              {% endcase %}
            </div>
          </div>
    
        {% endfor %}
      </div>
    
      {%- if section.settings.hero_layout == 'fixed_width' -%}
        </div>
      {%- endif -%}
    </div>
    
    <style>
      {% if section.settings.show_overlay %}
      .img_txt_custom .custom__item.custom__item--text .custom__item-inner--text {
        height: 100%;
        width: 100%;
        background-color:{{ section.settings.overlay_color | color_modify: 'alpha', section.settings.overlay_opacity }};
        display: block;
        padding: 15px 30px;
      }
      {% endif %}
     .img_txt_custom{
          padding:70px 0;
      }
      .img_cover{
          position:relative;
      }
      .overlay_div{
          position:absolute;
        left:0;
        right:0;
        top:0;
        bottom:0;
        margin:auto;
        width:100%;
        height:100%;
      }
      .opa_nil{opacity: 0;}
      .opa_low{opacity: 0.2;}
      .opa_midium{opacity: 0.4;}
      .opa_high{opacity: 0.6;}
      .opa_veryhigh{opacity: 0.8;}
      .opa_solid{opacity: 1;}
      
      .block-right_position {
        right: 55px;
        margin-right: 0;
    }
       .block-left_position {
        left: 55px;
        margin-left: 0;
    }
       .block-center_position {
        left: 0;
        right: 0;
        margin:0 auto;
    }
      .w_quarter-width{
          width:40%;
        margin: 0 auto;
      }
      .w_half-width{
          width:70%;
        margin: 0 auto;
      }
      .w_full-width{
          width:100%
      }
      .overlap_sec{
          position:relative;
        width:100%;
        float:left;
      }
      .txt_overlap_sec .overlap_sec{
          margin:0;
        width:100%;
      }
     /* .txt_overlap_sec {
        position: absolute;
        top: 730px;
        bottom: 0;
        left: 0;
        right: 0;
        margin: auto;
        z-index: 9;
        transform: translateY(-130px);
    }  */
      .txt_overlap_sec{
          position:relative;
        padding:0;
       /* height: 600px;*/
        width: 100%;
        overflow: hidden;
      }  
    /*.txt_overlap_sec .page-width {
        width: 100%;
        max-width: 100%;
        padding: 0;
        margin: 0;
    }*/
      .txt_overlap_sec .overlap_sec .custom__item.custom__item--image .custom__item-inner--image,.txt_overlap_sec .overlap_sec .custom__item.custom__item--image img {
        max-width: 100% !important;
        max-height: inherit !important;
      }
    .txt_overlap_sec .custom__item.custom__item--image{
        padding:0;  
        margin:0;
    }
     .txt_overlap_sec .custom__item.custom__item--text {
        position: absolute;
      /*  top: 50%;*/
        z-index: 1;
      /*  transform: translateY(-120px);*/
        padding: 0;
    }
      .header_sec{
          z-index:3 !important;
      }
      .txt_overlap_sec .overlap_sec .custom__item.align--top {
        top: 15px;
    }
    .txt_overlap_sec .overlap_sec .custom__item.align--bottom {
      margin-bottom: 0;
      bottom: 15px;
    }
      @media screen and (max-width:749px){
       .txt_overlap_sec .custom__item-inner{
            text-align:center ;
        }
        
      }
      .txt_overlap_sec .custom__item.custom__item--text.block-left_position {
        float: none;
    }
    @media screen and (max-width:991px){
    .txt_overlap_sec .custom__item.custom__item--text {
      padding-left: 0;
      padding-right: 0;
    }
    .block-right_position {
        right: 30px;
        margin-right: 0;
    }
       .block-left_position {
        left: 30px;
        margin-left: 0;
    }
    }
    @media screen and (max-width:767px){
    .txt_overlap_sec .custom__item.custom__item--text {
      left: 0;
      right: 0;
      top: 0;
      bottom: 0;
      margin: 0;
    }
    }
    </style>
    
    
    {% for block in section.blocks %}
    <style>
       .color_{{block.id}} .rte-setting.rte p{
        color:{{block.settings.text_color}}
      }
    </style>
    {% endfor %}
    
    
